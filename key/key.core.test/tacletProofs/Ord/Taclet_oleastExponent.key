\proofScript"
rule allRight;
rule allRight;
rule  impRight;
rule andLeft;
cut '!(\exists Ord v_k;
   (  olt(v_i_0,oexp(v_j_0, oadd(v_k,o_1)))))';
rule notLeft;
rule hide_right
  formula='\exists Ord v_k;
   (  oleq(oexp(v_j_0, v_k), v_i_0)
    & olt(v_i_0, oexp(v_j_0, oadd(v_k, o_1))))';
instantiate hide var=v_k with=v_i_0;
rule oexpRightNondecreasing;
instantiate hide var=x with=v_j_0;
instantiate hide var=y with='oadd(v_i_0, o_1)' ;
rule impLeft;
tryclose branch;
rule oSucc;
instantiate hide var=nv with=v_i_0;
rule oleqolt_trans
  formula='oleq(oadd(v_i_0, o_1), oexp(v_j_0, oadd(v_i_0, o_1)))';
tryclose branch;
rule notRight;
rule least_number_principle
  formula='\exists Ord v_k; olt(v_i_0, oexp(v_j_0, oadd(v_k, o_1))) ';
rule exLeft;
rule andLeft;
instantiate hide var=v_k with=v_k_2;
rule andRight;
cut 'lim(v_k_2)';
cut '!olt(o_0,v_j_0)';
tryclose branch;
rule notRight;
rule oexp_DefLim
  formula='oleq(oexp(v_j_0, v_k_2), v_i_0)';
rule osupDef;
rule hide_left
 formula='\forall Ord nv1;
   (   olt(nv1, v_k_2)
    -> oleq(oexp(v_j_0, nv1),
            osup{Ord nv;}(v_k_2, oexp(v_j_0, nv))))';
instantiate hide var=nv2 with=v_i_0;
rule impLeft;
tryclose branch;
rule hide_right
  formula='oleq(osup{Ord nv;}(v_k_2, oexp(v_j_0, nv)), v_i_0)';
rule allRight;
rule impRight;
instantiate hide var=k with=nv1_1 ;
rule impLeft;
tryclose branch;
rule notLeft;
cut '!(oleq(oexp(v_j_0, oadd(nv1_1, o_1)),v_i_0))';
rule notLeft;
rule hide_right
  formula='oleq(oexp(v_j_0, nv1_1), v_i_0)';
rule hide_left
  formula='olt(v_i_0, oexp(v_j_0, oadd(v_k_2, o_1)))';
rule olt_total_Axiom;
tryclose branch;
rule notRight;
rule hide_right
  formula='olt(v_i_0, oexp(v_j_0, oadd(nv1_1, o_1)))';
rule hide_left
    formula='olt(v_i_0, oexp(v_j_0, oadd(v_k_2, o_1)))';
tryclose branch;
rule oexpRightMonotoneQ;
instantiate hide var=x  with= v_j_0;
instantiate hide var=y1 with=nv1_1 ;
instantiate hide var=y2 with='oadd(nv1_1,o_1)';
rule impLeft;
tryclose branch;
rule hide_left
  formula='olt(v_i_0, oexp(v_j_0, oadd(v_k_2, o_1)))';
rule olimDef;
rule andRight;
rule notRight;
tryclose branch;
rule notRight;
rule exLeft;
instantiate hide var=k with=nv_1;
rule impLeft;
rule hide_right
  formula='oleq(oexp(v_j_0, v_k_2), v_i_0)';
rule oSucc;
instantiate hide var=nv with=nv_1;
tryclose branch;
rule notLeft;
rule applyEq
  formula='oleq(oexp(v_j_0, v_k_2), v_i_0)' occ=2;
rule olt_total_Axiom;
tryclose;
