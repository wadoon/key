\proofScript"
rule allRight;
rule allRight;
rule oInd;
tryclose branch;
# Successor case (Base case finishes automatically)
rule hide_right
  formula='\forall Ord v_j;
   (   olt(o_0, v_k_0) & olt(v_i_0, v_j)
    -> olt(otimes(v_k_0, v_i_0), otimes(v_k_0, v_j)))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft;
rule olessPlusOne;
cut 'olt(otimes(v_k_0, v_i_0), otimes(v_k_0, oadd(v_j_3, o_1)))';
rule oaddStrictMonotone;
instantiate hide var=x with='otimes(v_k_0, v_i_0)' ;
instantiate hide var=y with=v_k_0;
rule impLeft
  formula='   !v_k_0 = o_0
 -> olt(otimes(v_k_0, v_i_0), oadd(otimes(v_k_0, v_i_0), v_k_0))';
tryclose branch;
rule impLeft;
rule andRight;
tryclose branch;
rule orLeft;
tryclose branch;
rule otimes_DefSucc;
rule oaddStrictMonotone;
instantiate hide var=x with='otimes(v_k_0, v_i_0)' ;
instantiate hide var=y  with=v_k_0;
tryclose branch;
rule otimes_DefSucc;
rule oaddStrictMonotone;
instantiate hide var=x with='otimes(v_k_0, v_i_0)' ;
instantiate hide var=y  with=v_k_0;
rule impLeft;
tryclose branch;
rule orLeft;
cut 'oleq(otimes(v_k_0, v_i_0), otimes(v_k_0, v_j_3))';
rule oleqAddLessRight;
instantiate hide  var=k with=v_k_0 ;
rule oleqolt_trans
 formula='oleq(oadd(otimes(v_k_0, v_i_0), v_k_0),
      oadd(otimes(v_k_0, v_j_3), v_k_0))';
tryclose branch;
# Limit case
rule hide_right
 formula='\forall Ord v_j;
   (   olt(o_0, v_k_0) & olt(v_i_0, v_j)
    -> olt(otimes(v_k_0, v_i_0), otimes(v_k_0, v_j)))';
rule allRight;
rule impRight;
rule andLeft;
rule impRight;
rule andLeft;
instantiate hide var=v_j with='oadd(v_i_0,o_1)';
rule impLeft;
rule olimDefEquiv;
rule andLeft;
tryclose branch;
rule impLeft;
tryclose branch;
rule otimes_DefLim
  formula='olt(otimes(v_k_0, v_i_0), otimes(v_k_0, nv1_7))' occ=1;
rule osupDef;
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1;
         (olt(nv1, nv1_7) -> oleq(otimes(v_k_0, nv1), nv2))
    -> oleq(osup{Ord nv;}(nv1_7, otimes(v_k_0, nv)), nv2))';
instantiate hide var=nv1 with='oadd(v_i_0, o_1)';
rule impLeft;
rule olimDefEquiv;
rule andLeft;
tryclose branch;
rule oleqolt_trans
  formula='oleq(otimes(v_k_0, oadd(v_i_0, o_1)),
      osup{Ord nv;}(nv1_7, otimes(v_k_0, nv)))';
tryclose;
"
