\proofScript "
macro split-prop;
cut 'f_i=o_0';
cut 'otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))=o_0';
cut 'osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))=o_0';
rule hide_left
  formula='f_i=o_0';
tryclose branch;
rule hide_right
  formula='  osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))
 = otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule hide_left
  formula='otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))) = o_0';
rule osupEQDef;
rule andRight;
rule allRight;
rule impRight;
tryclose branch;
# from now on !f_i=o_0 can be assumed
cut 'f_b=o_0';
cut  'osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))=o_0';
cut 'osup{Ord v_lambda;}(f_b, f_j(v_lambda))=o_0';
tryclose branch;
rule hide_right
  formula='  osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))
 = otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule hide_left
  formula='osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))) = o_0';
rule hide_right
  formula='f_i = o_0';
rule eqSymm
  formula='f_b = o_0';
rule applyEqReverse 
  formula='osup{Ord v_lambda;}(f_b, f_j(v_lambda)) = o_0' occ=1;
tryclose branch;
rule hide_right
  formula='osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))
 = otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule hide_right
  formula='f_i = o_0';
rule eqSymm
  formula='f_b = o_0';
rule applyEqReverse 
  formula='osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))) = o_0' occ=1;
tryclose branch;
#  from now on also !f_b=o_0 can be assumed
#  this took so long since proofScript is not good om equality reasoning
cut 'oleq(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
         otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))))
     & 
    oleq(otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))),
     osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
tryclose branch;
rule hide_right
  formula='  osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))
 = otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
# now equality has been replaced by two inequalities
rule andRight;
rule osupDef occ=0;
rule hide_left
 formula='\forall Ord nv1;
   (   olt(nv1, f_b)
    -> oleq(otimes(f_i, f_j(nv1)),
            osup{Ord v_lambda;}(f_b,
                                otimes(f_i, f_j(v_lambda)))))';
instantiate hide var=nv2 with='otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule impLeft;  
rule osupDef 
   formula='oleq(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
      otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))))'  occ=1;
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1;
         (olt(nv1, f_b) -> oleq(f_j(nv1), nv2))
    -> oleq(osup{Ord v_lambda;}(f_b, f_j(v_lambda)), nv2))';
rule hide_right
  formula='oleq(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
      otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))))';
rule allRight;
rule impRight;
instantiate hide var=nv1 with=nv1_1;
rule impLeft;
tryclose branch;
rule  otimesWeakMonotoneQ;
instantiate hide var=k with=f_i ;
instantiate hide var=i with='f_j(nv1_1)' ;
instantiate hide var=j with='osup{Ord v_lambda;}(f_b, f_j(v_lambda))' ;
rule impLeft;
tryclose branch;
# the first and easy inequaltity has been proved
cut '!lim(osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
# we first consider the case that 
# osup{Ord v_lambda;}(f_b, f_j(v_lambda)) is not a limit ordinal
rule notLeft;
rule olimDef;
rule andRight;
# subcase osup{Ord v_lambda;}(f_b, f_j(v_lambda)) is 0
rule notRight;
cut 'otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))=o_0';
rule hide_left
  formula='o_0 = osup{Ord v_lambda;}(f_b, f_j(v_lambda))';
rule oleq_zeroAxiom;
instantiate hide var=nv with='osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))';
rule eqSymm
 formula='otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))) = o_0';
rule  applyEqReverse 
  formula='oleq(otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))' occ=0;
tryclose branch;
# subcase osup{Ord v_lambda;}(f_b, f_j(v_lambda)) is a successor ordinal
rule notRight;
rule exLeft;
rule oSucc;
instantiate hide var=nv with=nv_1 ;
cut '!(olt(nv_1,osup{Ord v_lambda;}(f_b, f_j(v_lambda))))';
tryclose branch;
rule notRight;
rule osupDef
  formula='osup{Ord v_lambda;}(f_b, f_j(v_lambda)) = oadd(nv_1, o_1)';
rule hide_left
 formula='\forall Ord nv1;
   (   olt(nv1, f_b)
    -> oleq(f_j(nv1),
            osup{Ord v_lambda;}(f_b, f_j(v_lambda))))';
instantiate hide var=nv2 with=nv_1 ;
rule impLeft;
rule allRight;
rule impRight;
cut '!olt(nv_1,f_j(nv1_2))';
rule notLeft;
rule hide_right
   formula='oleq(otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
rule olt_total_Axiom;
tryclose branch;
rule notRight;
rule oLeastSucc;
instantiate hide var=nv  with=nv_1 ;
instantiate hide var=nv1 with='f_j(nv1_2)';
rule impLeft;
tryclose branch;
rule otimesWeakMonotoneQ;
instantiate hide var=k with=f_i ;
instantiate hide var=i with='oadd(nv_1, o_1)' ;
instantiate hide var=j with='f_j(nv1_2)';
rule impLeft;
tryclose branch;
rule osupDef
 formula='oleq(otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))' occ=1;
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1;
         (   olt(nv1, f_b)
          -> oleq(otimes(f_i, f_j(nv1)), nv2))
    -> oleq(osup{Ord v_lambda;}(f_b,
                                otimes(f_i, f_j(v_lambda))),
            nv2))';
instantiate hide var=nv1 with=nv1_2  ;
rule impLeft;
tryclose branch;
rule oleq_trans
  formula='oleq(otimes(f_i, f_j(nv1_2)),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
tryclose branch;
# now we consider the really difficult case that 
# osup{Ord v_lambda;}(f_b, f_j(v_lambda)) is a limit ordinal
rule notRight;
cut '!(olt(
     osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
     otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))
        ))';
rule notLeft;
rule olt_total_Axiom;
tryclose branch;
rule notRight;
rule hide_right
  formula='oleq(otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
# proof by contradiction starts here
rule otimes_DefLim
   formula='olt(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
     otimes(f_i, osup{Ord v_lambda;}(f_b, f_j(v_lambda))))';
rule osupDef
  formula='olt(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
     osup{Ord nv;}(osup{Ord v_lambda;}(f_b, f_j(v_lambda)),
                   otimes(f_i, nv)))' occ=1;
rule hide_left
  formula='\forall Ord nv1;
   (   olt(nv1, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))
    -> oleq(otimes(f_i, nv1),
            osup{Ord nv;}(osup{Ord v_lambda;}(f_b,
                                              f_j(v_lambda)),
                          otimes(f_i, nv))))';
instantiate hide var=nv2 with='osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda)))';
rule impLeft;
rule allRight;
rule impRight;
rule osupDef
  formula='olt(nv1_3, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule hide_left
  formula='\forall Ord nv1;
   (   olt(nv1, f_b)
    -> oleq(f_j(nv1),
            osup{Ord v_lambda;}(f_b, f_j(v_lambda))))';
instantiate hide var=nv2 with=nv1_3  ;
rule impLeft;
rule allRight;
rule impRight;
cut '!(olt(nv1_3,f_j(nv1_4)))';
rule notLeft;
rule olt_total_Axiom;
tryclose branch;
rule notRight;
rule otimesMonotoneQ;
instantiate hide var=k with=f_i ;
instantiate hide var=i with=nv1_3 ;
instantiate hide var=j with='f_j(nv1_4)' ;
rule impLeft;
rule andRight;
rule oleq_zeroAxiom;
instantiate hide var=nv with=f_i;
rule oleq_replace
  formula='oleq(o_0, f_i)';
rule orLeft;
tryclose branch;
rule hide_right
  formula='oleq(f_j(nv1_4), nv1_3)';
rule hide_left
  formula='olt(nv1_3, f_j(nv1_4))';
rule hide_left
  formula='olt(osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))),
     osup{Ord nv;}(osup{Ord v_lambda;}(f_b, f_j(v_lambda)),
                   otimes(f_i, nv)))';
rule hide_left
  formula='lim(osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule hide_left
  formula='olt(nv1_3, osup{Ord v_lambda;}(f_b, f_j(v_lambda)))';
rule osupDef
  formula='oleq(otimes(f_i, nv1_3),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1;
         (   olt(nv1, f_b)
          -> oleq(otimes(f_i, f_j(nv1)), nv2))
    -> oleq(osup{Ord v_lambda;}(f_b,
                                otimes(f_i, f_j(v_lambda))),
            nv2))';
instantiate hide var=nv1 with=nv1_4 ;
rule impLeft
auto steps=2;
tryclose branch;
rule oleqolt_trans
  formula='oleq(otimes(f_i, f_j(nv1_4)),
      osup{Ord v_lambda;}(f_b, otimes(f_i, f_j(v_lambda))))';
tryclose;
"

