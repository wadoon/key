\proofObligation "#Proof Obligation Settings
#Wed Jan 20 12:52:20 CET 2016
name=odivLim
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
macro split-prop;
rule odivQ;
instantiate hide var=i with=f_lambda ;
instantiate hide var=j with=omega ;
rule impLeft;
rule omegaDef1 occ=0;
rule olimDef
  formula='lim(omega)';
tryclose branch;
rule exLeft;
rule exLeft;
rule andLeft;
rule olimDefAdd
  formula='lim(f_lambda)';
rule andLeft;
cut '!r_1=o_0';
cut 'lim(r_1)';
tryclose branch;
rule olimDef
  formula='lim(r_1)';
rule andRight;
tryclose branch;
rule notRight;
rule notLeft
  formula='!\exists Ord nv; f_lambda = oadd(nv, o_1)';
rule exLeft;
instantiate hide var=nv with='oadd(otimes(omega, d_1), nv_1)';
rule hide_right
   formula='\exists Ord v_d; f_lambda = otimes(omega, v_d)';
rule hide_left
  formula='lim(f_lambda)';
rule hide_left
  formula='!o_0 = f_lambda';
rule hide_left
  formula='!r_1 = o_0';
rule hide_left
  formula='olt(r_1, omega)';
cut '!oadd(otimes(omega, d_1), r_1)
     =
     oadd(otimes(omega, d_1),oadd(nv_1, o_1) ))'; 
tryclose branch;
rule notRight;
rule hide_left
  formula='r_1 = oadd(nv_1, o_1)';
cut '!(oadd(otimes(omega, d_1), oadd(nv_1, o_1))
     =
     oadd(oadd(otimes(omega, d_1), nv_1), o_1))';
rule notLeft;
rule oadd_DefSucc
   formula='  oadd(otimes(omega, d_1), oadd(nv_1, o_1))
 = oadd(oadd(otimes(omega, d_1), nv_1), o_1)';
tryclose branch;
tryclose;
"
