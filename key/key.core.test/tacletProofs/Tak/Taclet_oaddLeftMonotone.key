\proofScript "
rule allRight;
rule oInd;
# Base case
tryclose branch;
# Successor case
rule hide_right
 formula='\forall Ord v_y; oleq(v_y, oadd(v_x_0, v_y))';
rule allRight;
rule impRight;
cut 'oleq(oadd(v_y_0, o_1), oadd(v_x_0, oadd(v_y_0, o_1)))';
tryclose branch;
rule oadd_DefSucc;
tryclose branch;
rule oSucc;
instantiate hide var=nv with='oadd(v_x_0, v_y_0)';
rule oltleq_trans;
rule oLeastSucc;
tryclose branch;
# Limit case
rule hide_right
 formula='\forall Ord v_y; oleq(v_y, oadd(v_x_0, v_y))';
rule allRight;
rule impRight;
rule andLeft;
rule oadd_DefLim;
rule osupDef;
rule oselfSup;
instantiate hide var=nv with=nv1_0;
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1; (olt(nv1, nv1_0) -> oleq(oadd(v_x_0, nv1), nv2))
    -> oleq(osup{Ord nv;}(nv1_0, oadd(v_x_0, nv)), nv2))';
rule impLeft;
tryclose branch;
rule osupDef
  formula='osup{Ord lambda;}(nv1_0, lambda) = nv1_0';
instantiate hide var=nv2 with='osup{Ord nv;}(nv1_0, oadd(v_x_0, nv))';
rule impLeft;
rule hide_left
  formula='\forall Ord nv1; (olt(nv1, nv1_0) -> oleq(nv1, osup{Ord lambda;}(nv1_0, lambda)))';
rule allRight;
rule impRight;
instantiate hide var=nv1 with=nv1_1;
rule impLeft;
tryclose branch;
rule oaddLeftMonotone;
instantiate hide var=x with=v_x_0 ;
instantiate hide var=y with=nv1_1 ;
rule oleq_trans
  formula='oleq(oadd(v_x_0, nv1_1), osup{Ord nv;}(nv1_0, oadd(v_x_0, nv)))';
tryclose;

"
