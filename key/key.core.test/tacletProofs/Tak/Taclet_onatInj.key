\proofScript "
rule  auto_int_induction_geqZero;
rule  auto_int_induction_geqZero;
rule impRight;
rule andLeft;
tryclose branch;
# Base case of inner induction closed
rule impRight;
rule impRight;
rule andLeft
  formula='0 <= 0 & 0 <= v_m_0 + 1';
rule andLeft;
rule impLeft;
tryclose branch;
rule impRight;
rule hide_left
  formula='0 <= 0';
rule hide_left
  formula='0 <= v_m_0 + 1';
rule hide_left
  formula='onat(0) = onat(v_m_0) -> 0 = v_m_0';
rule onatSuccDef;
instantiate hide var= n with=v_m_0;
rule impLeft;
tryclose branch;
tryclose  branch;
# base case of outer induction closed.
tryclose  branch;
rule impRight;
rule andLeft;
rule allRight;
rule impRight;
rule andLeft;
rule impRight;
rule onatSuccDef;
instantiate hide var=n with=v_n_0;
rule impLeft;
tryclose  branch;
cut 'v_m_0=0';
tryclose  branch;
instantiate hide var=v_m with='v_m_0-1';
rule impLeft;
tryclose  branch;
rule impLeft;
cut 'onat(v_m_0)=oadd(onat(v_m_0 - 1),o_1)';
cut 'oadd(onat(v_n_0), o_1) = oadd(onat(v_m_0 - 1), o_1)';
rule oAddOneInj;
tryclose  branch;
rule onatoadd;
instantiate hide var=n with= '(v_m_0 -1)' ;
instantiate hide var=m with= 1 ;
tryclose branch;
rule onatSuccDef;
tryclose branch;
tryclose;
"

