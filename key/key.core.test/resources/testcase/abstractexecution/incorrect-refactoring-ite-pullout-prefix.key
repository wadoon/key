\predicates {
  P;
}

\programVariables {
  boolean b;
}

\problem {
    \<{
      if (b) {
        abstract_program P;
        abstract_program Q1;
      } else {
        abstract_program P;
        abstract_program Q2;
      }
    }\> P
  <->
    \<{
      abstract_program P;
      if (b) {
        abstract_program Q1;
      } else {
        abstract_program Q2;
      }
    }\> P
}
