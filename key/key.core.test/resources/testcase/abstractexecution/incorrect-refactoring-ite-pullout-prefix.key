\predicates {
  P;
}

\programVariables {
  boolean b;
}

\problem {
    \<{
      if (b) {
        abstract-program P;
        abstract-program Q1;
      } else {
        abstract-program P;
        abstract-program Q2;
      }
    }\> P
  <->
    \<{
      abstract-program P;
      if (b) {
        abstract-program Q1;
      } else {
        abstract-program Q2;
      }
    }\> P
}
