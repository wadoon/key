\sorts {
    Enum;
}
\functions {
    \unique Enum EA;
    \unique Enum EB;
    \unique Enum EC;
}
\axioms {
    enum_induction {
        \schemaVar \formula phi;
        \schemaVar \variable Enum base;

        \find( ==> \forall base; phi )

        "base case": \replacewith( ==> {\subst base; EA} phi );
        "1. case": \replacewith( ==> (phi -> {\subst base; EB} phi )  );
        "2. case": \replacewith( ==> (phi -> {\subst base; EC} phi )  )
    };
    enum_ctor_split {
        \schemaVar \term Enum enum;
        \find(enum) \sameUpdateLevel
        \replacewith(EA) \add(enum = EA ==>);
        \replacewith(EB) \add(enum = EB ==>);
        \replacewith(EC) \add(enum = EC ==>)
    };
}
\problem {
	EA != EB
}
