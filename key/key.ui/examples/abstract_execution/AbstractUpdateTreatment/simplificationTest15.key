\functions {
  LocSet lsA;
  LocSet lsB;
  LocSet lsC;
  LocSet lsD;
}

\programVariables {
  int x;
}

\problem {
        disjoint(lsA,lsC)
      & disjoint(singletonPV(x),lsA)
      & disjoint(singletonPV(x),lsC)
    ->
      {U_P({\fun(lsA)}:={\fun(lsB)})||x:=17||U_Q({\fun(lsC)}:={\fun(lsD)})}value(allLocs) =
      {U_Q({\fun(lsC)}:={\fun(lsD)})||x:=17||U_P({\fun(lsA)}:={\fun(lsB)})}value(allLocs)
}
