\javaSource ".";

\functions {
   \unique LocSet localsPreProc;
   \unique LocSet outA;
   \unique LocSet outB;
   \unique LocSet localsLoopBodyA;
   \unique LocSet localsLoopBodyB;
   \unique LocSet localsPostProcA;
   \unique LocSet localsPostProcB;
   \unique LocSet localsPostProc;
   Object _result;
   Object[] _loopArgs;
   Throwable _exc;
   Object _obj;
   int _i;
   boolean loopBodyBReturns;
   boolean loopBodyBThrows;
   boolean postProcAReturns;
   boolean postProcAThrows;
}

\predicates {
   Post(Object, boolean);
   mutuallyExclusive4(boolean, boolean, boolean, boolean);
}

\programVariables {
   Object[] loopArgs;
   Object result;
   SplitLoopRefactoring objUnderTest;
   Object obj, o_anon, o_init;
   int i;
}

\rules {
  mutuallyExclusive4 {
    \schemaVar \term boolean t1, t2, t3, t4;
    \find (mutuallyExclusive4(t1, t2, t3, t4))
    \replacewith (
      (t1 = TRUE -> (!(t2 = TRUE) & !(t3 = TRUE) & !(t4 = TRUE))) &
      (t2 = TRUE -> (!(t1 = TRUE) & !(t3 = TRUE) & !(t4 = TRUE))) &
      (t3 = TRUE -> (!(t2 = TRUE) & !(t1 = TRUE) & !(t4 = TRUE))) &
      (t4 = TRUE -> (!(t2 = TRUE) & !(t3 = TRUE) & !(t1 = TRUE)))
    )
    \heuristics(concrete)
  };
}

\problem {
       measuredByEmpty
     & !(_loopArgs = null)
     & _loopArgs.length > 0
     & mutuallyExclusive4(loopBodyBReturns, loopBodyBThrows, postProcAReturns, postProcAThrows)
   ->
   (
         {result:=_result||loopArgs:=_loopArgs||obj:=_obj||i:=_i}
           \<{ try {
                 result = objUnderTest.before(result, loopArgs, obj, i)@SplitLoopRefactoring;
               } catch (Throwable t) {
                 result = t;
               }
             }\> Post(result, TRUE)<<AEPred>>
     <-> {result:=_result||loopArgs:=_loopArgs||obj:=_obj||i:=_i}
           \<{ try {
                 result = objUnderTest.after(result, loopArgs, obj, i)@SplitLoopRefactoring;
               } catch (Throwable t) {
                 result = t;
               }
             }\> Post(result, TRUE)<<AEPred>>
   )
}

\proofScript "script 'splitLoopRefactoring.script';"
