<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uka.ilkd.key.nui.view.CssStylerViewController">
	<left>
		<ListView fx:id="listView" />
	</left>
   <center>
      <VBox prefHeight="400.0" prefWidth="350.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="table" editable="true" prefHeight="208.0" prefWidth="350.0">
               <columns>
                  <TableColumn fx:id="propertyColumn" editable="false" prefWidth="138.0" text="Property" />
                  <TableColumn fx:id="valueColumn" editable="true" prefWidth="211.0" text="Value" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <VBox prefHeight="159.0" prefWidth="350.0">
               <children>
                  <HBox prefHeight="35.0" prefWidth="340.0" spacing="20.0">
                     <children>
                        <Label prefHeight="31.0" prefWidth="145.0" text="Helping you pick a color:" />
                        <ColorPicker promptText="Color Helper">
                           <HBox.margin>
                              <Insets top="5.0" />
                           </HBox.margin></ColorPicker>
                     </children>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </HBox>
                  <WebView fx:id="previewWeb" prefHeight="124.0" prefWidth="350.0" />
               </children>
            </VBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="32.0" prefWidth="330.0" spacing="20.0">
               <children>
                  <Button fx:id="apply" disable="true" mnemonicParsing="false" onAction="#handleApply" text="Apply" />
                  <Button fx:id="reset" disable="true" mnemonicParsing="false" onAction="#handleReset" text="Reset" />
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
               </children>
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
