\proofScript "
macro split-prop;
rule oInd;
# Base Case
rule hide_right
 formula='\forall Ord v_k; oleq(oadd(f_i, v_k), oadd(f_j, v_k))';
tryclose branch;
# Successor Case
rule hide_right
 formula='\forall Ord v_k; oleq(oadd(f_i, v_k), oadd(f_j, v_k))';
rule allRight;
rule impRight;
rule oadd_DefSucc
  formula='oleq(oadd(f_i, oadd(v_k_3, o_1)), oadd(f_j, oadd(v_k_3, o_1)))' occ=0;
rule oadd_DefSucc;
rule oleq_replace
  formula='oleq(oadd(f_i, v_k_3), oadd(f_j, v_k_3))';
cut 'oadd(f_i, v_k_3) = oadd(f_j, v_k_3)';
tryclose branch;
rule orLeft;
rule oSucc;
instantiate hide var=nv with='oadd(f_j, v_k_3)' ;
rule oLeastSucc;
instantiate hide var=nv  with='oadd(f_i, v_k_3)' ;
instantiate hide var=nv1 with='oadd(f_j, v_k_3) ';
tryclose branch;
# Limit Case
rule hide_right
 formula='\forall Ord v_k; oleq(oadd(f_i, v_k), oadd(f_j, v_k))';
rule allRight;
rule impRight;
rule andLeft;
rule oadd_DefLim
  formula='oleq(oadd(f_i, nv1_3), oadd(f_j, nv1_3))' occ=0;
rule oadd_DefLim;
rule osupDef
   formula='oleq(osup{Ord nv;}(nv1_3, oadd(f_i, nv)), osup{Ord nv;}(nv1_3, oadd(f_j, nv)))' occ=0;
rule osupDef
   formula='oleq(osup{Ord nv;}(nv1_3, oadd(f_i, nv)), osup{Ord nv;}(nv1_3, oadd(f_j, nv)))' occ=1;
rule hide_left
  formula='\forall Ord nv1; (olt(nv1, nv1_3) -> olt(oadd(f_i, nv1), osup{Ord nv;}(nv1_3, oadd(f_i, nv))))';
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1; (olt(nv1, nv1_3) -> olt(oadd(f_j, nv1), nv2))
    -> oleq(osup{Ord nv;}(nv1_3, oadd(f_j, nv)), nv2))';
instantiate hide var=nv2 with='osup{Ord nv;}(nv1_3, oadd(f_j, nv))';
rule impLeft;
rule allRight;
rule impRight;
instantiate hide var=nv1 with='nv1_4' ;
instantiate hide var=v_k with='nv1_4' ;
rule impLeft
  formula='olt(nv1_4, nv1_3) -> olt(oadd(f_j, nv1_4), osup{Ord nv;}(nv1_3, oadd(f_j, nv)))';
tryclose branch;
rule impLeft;
tryclose  branch;
rule oltleq_trans
  formula='olt(oadd(f_j, nv1_4), osup{Ord nv;}(nv1_3, oadd(f_j, nv)))';
tryclose;
"