\proofObligation "#Proof Obligation Settings
#Mon Nov 02 22:37:56 CET 2015
name=oaddLessOmega
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
macro split-prop;
cut '\forall Ord x;(\forall Ord y;(
      olt(x,omega) & olt(y,omega) -> olt(oadd(x,y),omega)))';
instantiate hide var=x with=f_i ;
instantiate hide var=y with=f_j ;
tryclose branch;
# first cut branch closed
rule hide_right
 formula='olt(oadd(f_i, f_j), omega)';
rule omegaDef1 occ=1;
rule allRight;
rule oInd;
# Base case
rule hide_right
 formula='\forall Ord y; (olt(x_1, omega) & olt(y, omega) -> olt(oadd(x_1, y), omega))';
tryclose branch;
# Successor case
rule hide_right
 formula='\forall Ord y; (olt(x_1, omega) & olt(y, omega) -> olt(oadd(x_1, y), omega))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft;
rule impLeft;
rule andRight;
tryclose branch;
rule oSucc;
instantiate hide var=nv with=y_4;
rule olt_trans
  formula='olt(oadd(y_4, o_1), omega)';
tryclose branch;
rule oadd_DefSucc;
rule olimDedekind;
instantiate hide var=i  with='oadd(x_1, y_4)' ;
tryclose branch;
# Limit case
rule hide_right
 formula='\forall Ord y; (olt(x_1, omega) & olt(y, omega) -> olt(oadd(x_1, y), omega))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft
  formula='olt(x_1, omega) & olt(nv1_4, omega)'  ;
rule andLeft;
rule omegaDefLeastInf;
instantiate hide var=j with=nv1_4 ;
rule impLeft;
# the next rule application does not work
# not unique assumes clause
rule hide_left
 formula='lim(omega)';
rule olimDedekind;
rule andRight;
rule oleq_zeroAxiom;
instantiate hide var=nv with=nv1_4 ;
rule oleq_replace;
rule olimDef
  formula='lim(nv1_4)';
tryclose branch;
rule oltleq_trans
  formula='olt(nv1_4, omega)' ;
tryclose;
"





