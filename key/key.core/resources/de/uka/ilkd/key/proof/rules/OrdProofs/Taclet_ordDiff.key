name=ordDiff
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
macro split-prop;
cut 'oleq(f_j,oadd(f_i,f_j))';
# first branch of top level cut
cut '\exists Ord ov;(oleq(f_j,oadd(f_i,ov)))';
# first branch of second level cut
rule least_number_principle
  formula='\exists Ord ov; oleq(f_j, oadd(f_i, ov))';
rule exLeft;
rule andLeft;
cut 'ov_0=o_0';
# case ov_0 = o_0
cut 'f_i=f_j';
instantiate hide var=v_k  with=o_0 ;
tryclose branch;
# case ov_0 is successor
cut '\exists Ord gamma;(ov_0=oadd(gamma,o_1))';
rule exLeft;
rule oSucc;
instantiate hide var=nv with=gamma_0;
instantiate hide var=k with=gamma_0;
rule impLeft;
tryclose branch;
rule olt_total_Axiom;
rule notLeft;
rule oleq_replace
  formula='oleq(f_j, oadd(f_i, gamma_0))';
instantiate hide var=nv with=f_j ;
instantiate hide var=nv1 with='oadd(f_i, gamma_0)';
rule orRight;
instantiate hide var=v_k with=ov_0 ;
rule orLeft;
cut 'oleq(oadd(oadd(f_i,gamma_0),o_1),f_j)';
tryclose branch;
rule oLeastSucc;
instantiate hide var=nv with='oadd(f_i, gamma_0)' ;
instantiate hide var=nv1 with= f_j;
rule impLeft;
tryclose branch;
cut 'oleq(oadd(f_i,oadd(gamma_0, o_1)), f_j)';
tryclose branch;
rule oadd_DefSucc
  formula='oleq(oadd(f_i, oadd(gamma_0, o_1)), f_j)';
tryclose branch;
# case ov_0 is limit ordinal
cut 'lim(ov_0)';
instantiate hide var=v_k with=ov_0;
cut 'oleq(oadd(f_i, ov_0),f_j)';
tryclose branch;
rule oadd_DefLim
  formula='oleq(oadd(f_i, ov_0), f_j)';
rule osupDef;
instantiate hide var=nv2 with=f_j ;
rule impLeft;
rule allRight;
rule impRight;
instantiate hide var=k with=nv1_0;
rule impLeft;
tryclose branch;
rule hide_left
 formula='\forall Ord nv1;
   (olt(nv1, ov_0) -> olt(oadd(f_i, nv1), osup{Ord nv;}(ov_0, oadd(f_i, nv))))';
rule notLeft;
rule oleq_replace
 formula='oleq(f_j, oadd(f_i, nv1_0))';
rule orRight;
rule olt_total_Axiom;
instantiate hide var=nv   with= f_j;
instantiate hide var=nv1  with='oadd(f_i, nv1_0)' ;
tryclose branch;
rule olimDef;
rule andRight;
tryclose branch;
# second branch of second level cut
# second branch of top level cut
cut 'oleq(o_0,f_i)';
rule oleqAddLessRight
  formula='oleq(o_0, f_i)' ;
instantiate hide var=k with=f_j ;
tryclose;
"
