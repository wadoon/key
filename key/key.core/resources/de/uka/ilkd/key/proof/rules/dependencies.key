 splitNoRaWEQ {
    \schemaVar \term LocSet EQ, loc1, loc2;
 
    \assumes (union(loc1,loc2) = EQ ==>)
    \find(noRaW(EQ)) \sameUpdateLevel
    \replacewith(noRaW(loc1) & noRaW(loc2))
  
 };
 
