	\schemaVar \update u1,u2;
	\schemaVar \term[rigid] LocSet loc;

	
	\find({u1||u2}noRaW(loc))
	
	\varcond(\noEventUpdate(u1))
	
	\replacewith({u2}noRaW(loc))
	
	\heuristics(userTaclets1)
        
   };
   
