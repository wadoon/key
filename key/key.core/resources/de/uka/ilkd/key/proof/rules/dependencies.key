    \schemaVar \term LocSet EQ, loc1, loc2;
 
    \assumes (union(loc1,loc2) = EQ ==>)
    \find(noWaR(EQ)) \sameUpdateLevel
    \replacewith(noWaR(loc1) & noWaR(loc2))
  
