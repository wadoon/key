\proofScript "
rule allRight;
#copied from oaddLessOmega.key
rule oInd;
# Base case
rule hide_right
 formula='\forall Ord v_j; (olt(v_i_0, omega) & olt(v_j, omega) -> olt(oadd(v_i_0, v_j), omega))';
rule impRight;
rule andLeft;
tryclose branch;
# Successor case
rule hide_right
 formula='\forall Ord v_j; (olt(v_i_0, omega) & olt(v_j, omega) -> olt(oadd(v_i_0, v_j), omega))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft;
rule impLeft;
rule andRight;
tryclose branch;
rule oSucc;
instantiate hide var=nv with=v_j_3;
rule olt_trans
  formula='olt(oadd(v_j_3, o_1), omega)';
tryclose branch;
rule oadd_DefSucc;
rule omegaDef1 occ=0;
rule olimDedekind;
instantiate hide var=i  with='oadd(v_i_0, v_j_3)' ;
tryclose branch;
# Limit case
tryclose;
"





