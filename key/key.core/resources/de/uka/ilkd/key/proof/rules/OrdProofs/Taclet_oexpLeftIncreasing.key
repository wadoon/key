\proofObligation "#Proof Obligation Settings
#Sun Jan 31 16:49:36 CET 2016
name=oexpLeftIncreasing
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
rule allRight;
rule oInd;
# base case closes automatically
tryclose branch;
# successor case
rule hide_right
   formula='\forall Ord v_y;
   (   olt(o_1, v_x_0) & olt(o_1, v_y)
    -> olt(v_x_0, oexp(v_x_0, v_y)))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft;
cut 'v_y_3=o_1';
rule hide_left
 formula='olt(o_1, v_x_0) & olt(o_1, v_y_3)
 -> olt(v_x_0, oexp(v_x_0, v_y_3))';
rule hide_left
 formula='olt(o_1, oadd(v_y_3, o_1))';
rule oexp_DefSucc;
cut '!(oexp(v_x_0, v_y_3)=v_x_0)';
tryclose branch;
rule notRight;
cut 'olt(v_x_0,otimes(v_x_0,v_x_0))';
tryclose branch;
rule hide_right
  formula='olt(v_x_0, otimes(oexp(v_x_0, v_y_3), v_x_0))';
rule otimesMonotoneQ;
instantiate hide var=k with=v_x_0 ;
instantiate hide var=i with=o_1 ;
instantiate hide var=j with=v_x_0 ;
rule impLeft
rule andRight;
tryclose branch;
rule impLeft;
rule andRight;
tryclose branch;
rule oexp_DefSucc;
rule otimesWeakMonotoneQ;
instantiate hide var=k  with='oexp(v_x_0, v_y_3)' ;
instantiate hide var=i  with=o_1 ;
instantiate hide var=j  with=v_x_0 ;
rule impLeft;
tryclose branch;
cut '!(oleq(oexp(v_x_0, v_y_3),
        otimes(oexp(v_x_0, v_y_3), v_x_0)))';
tryclose branch;
rule notRight;
rule hide_left
  formula='oleq(otimes(oexp(v_x_0, v_y_3), o_1),
      otimes(oexp(v_x_0, v_y_3), v_x_0))';
rule oleqolt_trans
  formula='oleq(oexp(v_x_0, v_y_3),
      otimes(oexp(v_x_0, v_y_3), v_x_0))';
tryclose branch;
# limit case
rule hide_right
   formula='\forall Ord v_y;
   (   olt(o_1, v_x_0) & olt(o_1, v_y)
    -> olt(v_x_0, oexp(v_x_0, v_y)))';
rule allRight;
rule impRight;
rule andLeft;
rule impRight;
rule andLeft;
instantiate hide var=v_y with='oadd(o_1,o_1)';
rule impLeft;
rule hide_right
 formula='olt(v_x_0, oexp(v_x_0, nv1_8))';
rule olimDefEquiv;
rule andLeft;
tryclose branch;
rule impLeft;
tryclose branch;
cut '!(olt(o_0, v_x_0))';
tryclose branch;
rule notRight;
rule oexp_DefLim
  formula='olt(v_x_0, oexp(v_x_0, nv1_8))';
rule osupDef;
rule hide_left
  formula='\forall Ord nv2;
   (   \forall Ord nv1;
         (olt(nv1, nv1_8) -> oleq(oexp(v_x_0, nv1), nv2))
    -> oleq(osup{Ord nv;}(nv1_8, oexp(v_x_0, nv)), nv2))';
instantiate hide var=nv1 with='oadd(o_1, o_1)';
rule impLeft;
rule hide_right
  formula='olt(v_x_0, osup{Ord nv;}(nv1_8, oexp(v_x_0, nv)))';
rule hide_left
 formula='olt(v_x_0, oexp(v_x_0, oadd(o_1, o_1)))';
rule olimDefEquiv;
rule andLeft;
tryclose branch;
rule oleqolt_trans
  formula='oleq(oexp(v_x_0, oadd(o_1, o_1)),
      osup{Ord nv;}(nv1_8, oexp(v_x_0, nv)))';
tryclose;
"
