cut '!v_exp_2 + 1 > 0';
tryclose branch;
rule notRight;
rule lt_to_gt formula='1 < v_base_3';
cut 'v_goal_0 = 0';
rule powGreaterOne occ=1;
tryclose branch;
rule leq_to_geq formula='0 <= v_exp_2';
rule powExpSucc;
cut '!v_goal_0*v_base_3 < pow(v_base_3, v_exp_2)*v_base_3';
rule notLeft;
cut '!0<v_base_3';
tryclose branch;
rule notRight;
cut 'v_goal_0 + 1 <= v_goal_0 * v_base_3';
tryclose branch;
rule hide_right formula='v_goal_0 + 1 < pow(v_base_3, v_exp_2) * v_base_3';
rule hide_left formula='v_goal_0 * v_base_3 < pow(v_base_3, v_exp_2) * v_base_3';
cut 'v_goal_0 * 2 <= v_goal_0 * v_base_3'
tryclose branch;
tryclose branch;
rule hide_right formula='v_goal_0 + 1 <= v_goal_0 * v_base_3';
cut '2<= v_base_3';
rule multLessAux;
instantiate hide var='right' with='v_base_3';
instantiate hide var='left' with='2';
instantiate hide var='factor' with='v_goal_0';
tryclose branch;
tryclose;
