name=oltAddLessLeft
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
macro split-prop;
rule allRight;

cut '\forall Ord k;\forall Ord i;\forall Ord j;(olt(i,j) -> olt(oadd(k,i),oadd(k,j)))';
instantiate hide var=k with=v_k_0 ;
instantiate hide var=i with=f_i ;
instantiate hide var=j with=f_j ;
tryclose branch;
rule allRight;
rule allRight;
rule oInd;
# Base Case
rule hide_right
  formula='\forall Ord j; (olt(i_1, j) -> olt(oadd(k_1, i_1), oadd(k_1, j)))';
tryclose branch;
# Successor Case
rule hide_right
  formula='\forall Ord j; (olt(i_1, j) -> olt(oadd(k_1, i_1), oadd(k_1, j)))';
rule allRight;
rule impRight;
rule impRight;
rule oadd_DefSucc;
tryclose branch;
# Limit Case
rule hide_right
  formula='\forall Ord j; (olt(i_1, j) -> olt(oadd(k_1, i_1), oadd(k_1, j)))';
rule allRight;
rule impRight;
rule impRight;
rule andLeft;
rule oadd_DefLim
  formula='olt(oadd(k_1, i_1), oadd(k_1, nv1_2))' occ=1;
instantiate hide var=j with='oadd(i_1,o_1)';
rule oSucc;
instantiate hide var=nv with='i_1';
cut 'olt(oadd(i_1, o_1),nv1_2)';
# first cut case
rule impLeft;
tryclose branch;
rule impLeft;
tryclose branch;
rule osupDef;
tryclose branch;
# second cut case
rule olimDef;
rule andLeft;
rule notLeft
 formula='!\exists Ord nv; nv1_2 = oadd(nv, o_1)';
rule olt_total_Axiom;
instantiate hide var=nv with='oadd(i_1, o_1)';
instantiate hide var=nv1 with=nv1_2;
rule orLeft;
tryclose branch;
rule oLeastSucc;
tryclose;
"
