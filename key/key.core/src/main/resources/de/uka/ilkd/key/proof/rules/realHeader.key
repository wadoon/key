\sorts{

  real;
}

\functions
{
  real R(numbers, numbers);

  // Arithmetic operations transformed differently depending on chosen semantics
  // real 0
  // real 1
  real rNeg(real);
  real rAdd(real, real);
  real rMul(real, real);
  real rSub(real, real);
  real rdiv(real, real);
}

\predicates
{
  rlt(real, real);
  rlgt(real, real);
  rge(real, real);
  rgt(real, real);
}
