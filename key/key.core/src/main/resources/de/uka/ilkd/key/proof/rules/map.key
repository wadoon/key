/** Abstract datatype of (untyped) partial maps.
 * @author Daniel Bruns
 */

\sorts {
    Map;
}


\functions {
    // getters
    any mapGet(Map, any);
    \unique any mapUndef;
    
    // constructors
    Map mapForeach{true,true}(boolean, any);
    Map mapEmpty;
    Map mapSingleton(any, any);
    Map mapOverride(Map, Map);
    Map seq2map(Seq);
    Map mapUpdate(Map, any, any);
    Map mapRemove(Map, any);
}

\predicates {
    inDomain(Map, any);
    inDomainImpliesCreated(Map);
}


