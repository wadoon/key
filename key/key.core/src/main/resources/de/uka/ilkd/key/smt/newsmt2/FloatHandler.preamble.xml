<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

    <comment>These are the preamble snippets needed for handling floating point numbers.
    Older SMT solvers do not understand "Float32" and "Float64" such that these
    are printed out in long.</comment>

    <entry key="float.axioms">
(assert (forall ((f (_ FloatingPoint  8  24))) (= (u2f (f2u f)) f)))
(assert (forall ((x U)) (=> (= (typeof x) sort_float) (= (f2u (u2f x)) x))))
(assert (forall ((x U)) (=> (instanceof x sort_float) (= (typeof x ) sort_float))))
(assert (forall ((f (_ FloatingPoint  8  24))) (= (typeof (f2u f)) sort_float)))

(assert (forall ((d (_ FloatingPoint  11  53))) (= (u2d (d2u d)) d)))
(assert (forall ((x U)) (=> (= (typeof x) sort_double) (= (d2u (u2d x)) x))))
(assert (forall ((x U)) (=> (instanceof x sort_double) (= (typeof x ) sort_double))))
(assert (forall ((d (_ FloatingPoint  11  53))) (= (typeof (d2u d)) sort_double)))
    </entry>

    <entry key="float.decls">
(declare-fun u2f (U) (_ FloatingPoint  8  24))
(declare-fun f2u ((_ FloatingPoint  8  24)) U)
;(declare-const sort_float T)

(declare-fun u2d (U) (_ FloatingPoint  11  53))
(declare-fun d2u ((_ FloatingPoint  11  53)) U)
;(declare-const sort_double T)
    </entry>

    <entry key="sinDouble.axioms">
        <!-- TODO this is not mentioned in the documentation.-->
        (assert (forall ((a (_ FloatingPoint  11  53))) (=> (and (not (fp.isNaN a)) (not (fp.isInfinite a))) (and (fp.leq (sinDouble a) (fp #b0 #b01111111111
        #b0000000000000000000000000000000000000000000000000000))(fp.geq (sinDouble a) (fp #b1 #b01111111111
        #b0000000000000000000000000000000000000000000000000000))))))

        <!-- If the argument is NaN or an infinity, then the result is NaN.-->
        (assert (forall ((a (_ FloatingPoint  11  53))) (=> (or (fp.isNaN a)(fp.isInfinite a)) (fp.isNaN (sinDouble a)))))

        <!-- If the argument is zero, then the result is a zero with the same sign as the argument.-->
        (assert (forall ((a (_ FloatingPoint  11  53))) (=> (fp.eq a (fp #b0 #b00000000000
        #b0000000000000000000000000000000000000000000000000000))(fp.eq (sinDouble a) (fp #b0 #b00000000000
        #b0000000000000000000000000000000000000000000000000000)))))
        (assert (forall ((a (_ FloatingPoint  11  53))) (=> (fp.eq a (fp #b1 #b00000000000
        #b0000000000000000000000000000000000000000000000000000))(fp.eq (sinDouble a) (fp #b1 #b00000000000
        #b0000000000000000000000000000000000000000000000000000)))))

    </entry>

    <entry key="sinDouble.decls">
        (declare-fun sinDouble ((_ FloatingPoint  11  53)) (_ FloatingPoint  11  53))
    </entry>

