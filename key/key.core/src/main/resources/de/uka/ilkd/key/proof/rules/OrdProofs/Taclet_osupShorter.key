name=osupShorter
class=de.uka.ilkd.key.taclettranslation.lemma.TacletProofObligationInput
";

\proofScript "
macro split-prop;
rule osupDef occ=0;
rule hide_left
  formula='\forall Ord nv1;
   (   olt(nv1, f_b1)
    -> oleq(f_i(nv1), osup{Ord v_lambda;}(f_b1, f_i(v_lambda))))'; 
instantiate hide  var=nv2 with='osup{Ord v_lambda;}(f_b2, f_i(v_lambda))';
rule impLeft;
rule hide_right
 formula='oleq(osup{Ord v_lambda;}(f_b1, f_i(v_lambda)),osup{Ord v_lambda;}(f_b2, f_i(v_lambda)))';
rule allRight;
rule impRight;
rule oleq_replace
  formula='oleq(f_b1, f_b2)';
rule osupDef;
rule hide_left
 formula='\forall Ord nv2;
   (   \forall Ord nv1; (olt(nv1, f_b2) -> oleq(f_i(nv1), nv2))
    -> oleq(osup{Ord v_lambda;}(f_b2, f_i(v_lambda)), nv2))';
instantiate hide var=nv1 with=nv1_0 ;
rule impLeft;
rule orLeft;
rule olt_trans
 formula='olt(f_b1, f_b2)';
tryclose;
"
