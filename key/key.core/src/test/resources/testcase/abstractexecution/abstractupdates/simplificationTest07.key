\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\predicates {
  p(boolean,boolean);
}

\programVariables {
  boolean x;
  boolean y;
  boolean w;
}

\problem {
        {y:=x}
          {U_P(hasTo(singletonPV(y)),hasTo(singletonPV(w)):=y,w)}
            {x:=y}p(x,w)
    <-> {U_P(hasTo(singletonPV(x)),hasTo(singletonPV(w)):=x,w)}p(x,w)
}
