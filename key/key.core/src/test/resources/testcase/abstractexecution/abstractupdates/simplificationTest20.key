\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
  LocSet frameP;
  LocSet frameQ;
  Heap anonFunc;
  Heap subHeap;
}

\problem {
      disjoint(frameP, frameQ)
    ->
        {heap:=anon(subHeap, frameQ, anonFunc)}
          value(frameP)
      = {heap:=subHeap}
          value(frameP)
}
