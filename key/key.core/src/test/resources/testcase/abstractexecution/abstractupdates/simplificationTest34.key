\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
  LocSet locs;
  LocSet frame(int);
  int j_0;
}

\programVariables {
  int j;
  int k;
  int x;
}

\problem {
  !pvElementOf(PV(x), frame(k)) &
  !pvElementOf(PV(j), frame(k)) &
  !pvElementOf(PV(j), frame(j_0)) ->
   {j:=j_0 || U_P(frame(k):=value(singletonPV(PV(x))))}{U_P(frame(j):=value(singletonPV(PV(x))))}value(frame(j_0))
 = {U_P(frame(k):=value(singletonPV(PV(x)))) || U_P(frame(j_0):=value(singletonPV(PV(x))))}value(frame(j_0))
}
