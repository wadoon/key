\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
  LocSet locSet1;
  LocSet locSet2;
  LocSet locSet3;
  any footprint;
}

\problem {
      disjoint(locSet1, locSet3) &
      disjoint(locSet2, locSet3)
    ->
        {heap:={U_P(locSet1, locSet2:=footprint)}heap}value(locSet3)
      = value(locSet3)
}
