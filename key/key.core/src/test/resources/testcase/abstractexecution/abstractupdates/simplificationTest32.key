\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
  LocSet frame(int);
  int j_0;
}

\programVariables {
  int j;
  int k;
  int x;
}

\problem {
    !pvElementOf(PV(j), frame(j_0))
  & !pvElementOf(PV(j), frame(k))
  & !pvElementOf(PV(x), frame(k))
  & disjoint(frame(k), frame(j_0)) ->
      {j:=j_0||U_P(frame(k):=singletonPV(x))} {U_P(frame(j):=singletonPV(x))} value(frame(j_0))
    =                                         {U_P(frame(j_0):=singletonPV(x))} value(frame(j_0))
}
