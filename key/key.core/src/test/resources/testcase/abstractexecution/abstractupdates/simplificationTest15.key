\functions {
  LocSet lsA;
  LocSet lsB;
  LocSet lsC;
  LocSet lsD;
}

\programVariables {
  int x;
}

\problem {
        disjoint(lsA,lsC)
      & disjoint(singletonPV(x),lsA)
      & disjoint(singletonPV(x),lsC)
    ->
      {U_P(lsA:=lsB)||x:=17||U_Q(lsC:=lsD)}value(allLocs) =
      {U_Q(lsC:=lsD)||x:=17||U_P(lsA:=lsB)}value(allLocs)
}
