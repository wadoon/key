\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
  LocSet frameP;
  LocSet frameQ;
  LocSet footprintP;
  LocSet footprintQ;
}

\programVariables {
  Heap heap;
}

\problem {
      disjoint(frameP, frameQ) &
      disjoint(frameP, footprintQ) &
      disjoint(frameQ, footprintP)
    ->
      {U_P(frameP:=footprintP)} {U_Q(frameQ:=footprintQ)} heap =
      {U_Q(frameQ:=footprintQ)} {U_P(frameP:=footprintP)} heap
}
