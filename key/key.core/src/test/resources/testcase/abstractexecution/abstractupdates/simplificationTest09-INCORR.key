\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\functions {
   \unique \nonRigid LocSet someUnknownLocset;
}

\predicates {
   p(int);
}

\programVariables {
  int x;
}

\problem {
  {x:=1}{U_P(hasTo(singletonPV(x)):=someUnknownLocset)}p(x) <->
  {U_P(hasTo(singletonPV(x)):=someUnknownLocset)}p(x)
}
