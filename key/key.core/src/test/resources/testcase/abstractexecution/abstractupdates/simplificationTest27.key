\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\predicates {
  p(int, int);
}

\programVariables {
  int w;
  int x;
  int y;
}

\problem {
      {y:=x}
        {U_P(hasTo(singletonPV(y)):=singletonPV(y),singletonPV(w))}
          {x:=y} p(x,w)
  <-> {U_P(hasTo(singletonPV(x)):=singletonPV(x),singletonPV(w))} p(x,w)
}
