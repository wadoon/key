// This is a test for the strategies, which have to suitably
// instantiate the quantified premise (i --> j).

\functions {
  LocSet frame(int);
}

\programVariables {
  int i;
  int j;
  int x;
}

\problem {
    (\forall int i; !(pvElementOf(PV(x), frame(i)))) ->
      (  {U_P(frame(j):=singletonPV(x))} value(singletonPV(x))
       =                                 value(singletonPV(x)))
}
