\settings {"[StrategyProperty]OSS_OPTIONS_KEY=OSS_OFF"}

\predicates {
  Q(int,int,int,int);
}

\programVariables {
  int x; int y; int z; int w;
}

\problem {
  {U_P(singletonPV(x):=singletonPV(y))}{U_Q(singletonPV(w):=singletonPV(z))}Q(x,y,w,z) <->
  {U_Q(singletonPV(w):=singletonPV(z))}{U_P(singletonPV(x):=singletonPV(y))}Q(x,y,w,z)
}
