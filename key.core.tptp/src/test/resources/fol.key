\sorts {
	S;
}

\predicates {
	country(S);
	beautiful(S);
	big_city(S);
	city(S);
	has_crime(S);
}

\functions {
    S usa;
	S capital_of(S);
}

\problem {
	country(usa) &
	(\forall S C; (country(C) -> (beautiful(capital_of(C)) & big_city(capital_of(C))))) &
	(\exists S C; (city(C) & C = capital_of(usa))) &
	(\forall S C; (big_city(C) -> has_crime(C))) &
	(\forall S C; (big_city(C) -> city(C)))
->
	(\exists S C; (has_crime(C) & (beautiful(C) & city(C))))
}
