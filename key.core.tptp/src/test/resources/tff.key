\sorts {
    human;
    grade;
}

\functions {
    human john;
    grade a;
    grade f;
    grade grade_of(human);
}

\predicates {
    created_equal(human, human);
}

\problems {
    \forall human H1; (\forall human H2; (created_equal(H1,H2))) &
    grade_of(john) = f &
    \exists human H; ( grade_of(H) = a ) &
    a != f
    ->
    \exists human H; H != john & created_equal(H,john)
}