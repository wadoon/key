\schemaVariables {
  \term double d1, d2;
}


\rules(programRules:Java){

 ltTofloat {
   \find (lt(d1,d2))
   \replacewith( javaLtFloat(d1,d2) )
   \heuristics(javaFloatSemantics)
 };

 leqTofloat {
   \find (leq(d1,d2))
   \replacewith( javaLeqFloat(d1,d2) )
   \heuristics(javaFloatSemantics)
 };

 gtTofloat {
   \find (gt(d1,d2))
   \replacewith( javaGtFloat(d1,d2) )
   \heuristics(javaFloatSemantics)
 };

 geqTofloat {
   \find (geq(d1,d2))
   \replacewith( javaGeqFloat(d1,d2) )
   \heuristics(javaFloatSemantics)
 };
}
