\sorts
{
  fpInterval;
  floatInterval \extends fpInterval;
}

\functions
{
  
  //A point interval of a float
  floatInterval FTI(float);

  //Functions for getting the upper/lower end of intervals
  float ivMin(fpInterval);
  float ivMax(fpInterval);
  
}

\predicates
{

  //The same as float predicates,
  //only difference is these can't be transformed to use intervals
  //(since they are already transformed once)
  interLt(float, float);
  interGt(float, float);
  interLeq(float, float);
  interGeq(float, float);
}
