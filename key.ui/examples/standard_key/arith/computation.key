\programVariables {
	int x, y, z;
}

\functions {
	int px; int py; int pz;
}


\problem {
	inInt(x) -> inInt(y) -> inInt(z) ->
	x = px -> y = py -> z = pz ->
        y != 1 ->
	\<{
		x *= x;
		y = y + x - 1;
		z = x - y;
		y *= x;
		y /= 3;
		z = -z;
		z = (-34) / z;
	}\> (  x = moduloInt(px*px)
             & y = moduloInt(jdiv(moduloInt((py + px*px - 1)*px*px), 3))
             & z = moduloInt(jdiv(-34, moduloInt(py-1))))
}
